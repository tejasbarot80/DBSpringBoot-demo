<body>
<div class="wrapper fadeInDown">
  <div id="formContent">
    <!-- Tabs Titles -->

    <!-- Icon -->
    <div class="fadeIn first">
      <img src="http://danielzawadzki.com/codepen/01/icon.svg" id="icon" alt="User Icon" />
    </div>

    <!-- Login Form -->
    <form action="reg" method="post">
      <input type="text" id="login" class="fadeIn second" name="sname" placeholder="name">
      <input type="text" id="password" class="fadeIn third" name="semail" placeholder="email">
      <input type="submit" class="fadeIn fourth" value="Register">
    </form>

   

  </div>
